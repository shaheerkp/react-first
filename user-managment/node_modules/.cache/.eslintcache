n        alert(res.data.mes);\n      }\n    });\n  };\n\n  return (\n    <div className=\"app\">\n      <div className=\"mt-5 mainHeading\">\n        <h1>ToDo List</h1>\n      </div>\n      <div className=\"subHeading\">\n        <br />\n        <h2>Whoop, it's Wednesday üåù ‚òï </h2>\n      </div>\n      <div className=\"input\">\n        <input\n          value={todo}\n          type=\"text\"\n          onChange={(e) => setTodo(e.target.value)}\n          placeholder=\"üñäÔ∏è Add item...\"\n        />\n        <i onClick={addTodo} className=\"fas fa-plus\"></i>\n      </div>\n      <div className=\"todos\">\n        {todoss.map((value) => {\n          return (\n            <div className=\"todo\">\n              <div className=\"left\">\n                <input  checked={value.iscompleted} onChange={(e)=>{\n                  axios.post(\"http://localhost:4000/changestatus\",{date:value.date,userid:userId,iscompleted:value.iscompleted})\n\n                }} type=\"checkbox\" name=\"\" id=\"\" />\n                <p style={value.iscompleted?{textDecoration:\"line-through\"}:null} >{value.task}</p>\n              </div>\n              <div className=\"right\">\n                <i className=\"fas fa-times\"></i>\n              </div>\n            </div>\n          );\n        })}\n      </div>\n    </div>\n  );\n}\n\nexport default Todo;\n",{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":26,"column":6,"nodeType":"41","endLine":26,"endColumn":8,"suggestions":"42"},{"ruleId":"39","severity":1,"message":"40","line":31,"column":6,"nodeType":"41","endLine":31,"endColumn":8,"suggestions":"43"},{"ruleId":"44","severity":1,"message":"45","line":39,"column":9,"nodeType":"46","endLine":39,"endColumn":47},{"ruleId":"39","severity":1,"message":"47","line":14,"column":14,"nodeType":"48","endLine":14,"endColumn":22},"no-native-reassign",["49"],"no-negated-in-lhs",["50"],"react-hooks/exhaustive-deps","React Hook